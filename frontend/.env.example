# Environment Configuration Example
# Copy this file to .env and update with your actual values

# App Configuration
VITE_APP_NAME="ZuriRent"
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Blockchain Configuration
VITE_SCROLL_RPC_URL=https://sepolia-rpc.scroll.io
VITE_CONTRACT_ADDRESS_RENTAL_AGREEMENT=0x...
VITE_CONTRACT_ADDRESS_PROPERTY_REGISTRY=0x...

# API Configuration
VITE_API_BASE_URL=https://api.zurirent.xyz
VITE_IPFS_GATEWAY=https://ipfs.io/ipfs

# Third-party Services
VITE_ANALYTICS_ID=your-analytics-id
VITE_SENTRY_DSN=your-sentry-dsn
VITE_ALCHEMY_API_KEY=your-alchemy-key

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE=true

# Security
VITE_CSRF_SECRET=your-csrf-secret